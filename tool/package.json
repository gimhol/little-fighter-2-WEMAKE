{
  "name": "tool",
  "version": "1.0.0",
  "description": "",
  "main": "dist/tool.cjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc & rollup -c rollup.config.mjs",
    "start": "npm run build & node ./",
    "help": "npm run build & ./node ./ -- help",
    "dat-2-txt": "npm run build & node ./ -- dat-2-txt",
    "exe:win:x64": "bun build ./src/index.ts --compile --minify --sourcemap --bytecode --target=bun-windows-x64 --outfile ./exe/win/x64/lf2w-tool.exe",
    "exe:mac:x64": "bun build ./src/index.ts --compile --minify --sourcemap --bytecode --target=bun-darwin-x64 --outfile ./exe/mac/x64/lf2w-tool",
    "exe:mac:arm64": "bun build ./src/index.ts --compile --minify --sourcemap --bytecode --target=bun-darwin-arm64 --outfile ./exe/mac/arm64/lf2w-tool",
    "exe:linux:x64": "bun build ./src/index.ts --compile --minify --sourcemap --bytecode --target=bun-linux-x64 --outfile ./exe/linux/arm64/lf2w-tool",
    "exe:linux:arm64": "bun build ./src/index.ts --compile --minify --sourcemap --bytecode --target=bun-linux-arm64 --outfile ./exe/linux/arm64/lf2w-tool",
    "exe:all":"npm run exe:win:x64 && npm run exe:mac:x64 && npm run exe:mac:arm64 && npm run exe:linux:x64 && npm run exe:linux:arm64"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "command-exists": "^1.2.9",
    "compressing": "^2.0.0",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/command-exists": "^1.2.3",
    "@types/node": "^20.11.19",
    "rollup": "^4.55.1",
    "tslib": "^2.6.2",
    "typescript": "^5.5.2"
  }
}