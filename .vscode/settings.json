{
  "eslint.nodeEnv": "development",
  "editor.minimap.autohide": "none",
  "editor.rulers": [
    80
  ],
  "files.eol": "\n",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "json5.schemas": [
    {
      "fileMatch": [
        "*.ui.json5",
      ],
      "url": "/.vscode/ui.schema.json5"
    },
    {
      "fileMatch": [
        "*.index.json5"
      ],
      "url": "/.vscode/schema/idx.schema.json5"
    },
    {
      "fileMatch": [
        "*.bg.json5"
      ],
      "url": "/.vscode/schema/bg.schema.json5"
    },
    {
      "fileMatch": [
        "*.obj.json5"
      ],
      "url": "/.vscode/schema/obj.schema.json5"
    }
  ]
}